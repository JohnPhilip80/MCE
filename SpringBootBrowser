Include the Thymeleaf Dependency in pom.xml file.
  <dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-thymeleaf</artifactId>
	</dependency>

create 'student' folder under src/main/resources/templates.
create 'home-page.html' file under templates folder.
<html>
  <head>
    <title>MCE Students</title>
  </head>
  <body>
    <h1>Welcome to MCE</h1>
  </body>
</html>

create StudentBrowserController class in edu.mce.john.studentdb.controllers package.
package edu.mce.john.studentdb.controllers;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
@RequestMapping("/student/ui")
public class StudentBrowserController {
	@GetMapping({"","/","homepage"})
	public String homePage() {
		return "/student/home-page.html";
	}
}
Run and test in browser with the url http://localhost:8090/student/ui

